{
    "info": {
        "name": "Cloud Printing API",
        "description": "Complete API collection for Cloud Printing Backend",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "variable": [
        {
            "key": "baseUrl",
            "value": "http://localhost:8081",
            "type": "string"
